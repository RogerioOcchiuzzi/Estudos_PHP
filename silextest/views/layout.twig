<html>
<head>
    <title>Silex Example</title>
</head>
<body>
    {% if errors is defined %}
        <p>Something went wrong!</p>
        <ul>
        {% for error in errors %}
            <li>{{ error }}</li>
        {% endfor %}
        </ul>
    {% endif %}
    {% block content %}
    {% endblock %}
</body>
</html>